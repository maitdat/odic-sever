@model CodeFlowViewResponseModel
@{
    Layout = null;
}

<html>
    <head>

    </head>
<body onload="javascript:document.forms[0].submit()">
        <form action="@Model.RedirectUri" method="post" enctype="application/x-www-form-urlencoded">
            <input type="hidden" name="code" value="@Model.Code"/>
            <input type="hidden" name="state" value="@Model.State"/>
        </form>
    </body>

</html>